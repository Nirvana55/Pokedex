ght: 40px;\n      border-radius: 5px;\n      border: 1px solid var(--pokemonBlue);\n      padding: 10px;\n      outline: none;\n      font-family: \"Poppins\", sans-serif;\n      color: grey; }\n      @media (min-width: 768px) {\n        #header .container #form #inputPokemon {\n          width: 70%;\n          margin-left: 2.5rem;\n          height: 43px; } }\n      @media (min-width: 1024px) {\n        #header .container #form #inputPokemon {\n          width: 55%; } }\n      #header .container #form #inputPokemon::placeholder {\n        font-size: 14px; }\n      #header .container #form #inputPokemon:focus {\n        border: 2px solid var(--pokemonBlue);\n        box-shadow: 0px 0px 2px 1px var(--pokemonBlue); }\n  #header .container #form:focus + .btnSearch {\n    border: 1px solid var(--pokemonBlue); }\n  #header .container .btnSearch {\n    position: absolute;\n    height: 35px;\n    width: 40px;\n    font-size: 16px;\n    right: 10.5%;\n    top: 3%;\n    border-radius: 4px;\n    background: var(--pokemonYellow);\n    box-shadow: inset 1px 1px grey;\n    border: 1px solid white;\n    border-left: none;\n    border-right: none;\n    color: var(--pokemonBlue);\n    cursor: pointer;\n    padding: 6px; }\n    @media (min-width: 768px) {\n      #header .container .btnSearch {\n        height: 39px;\n        right: 12.9%; } }\n    @media (min-width: 1024px) {\n      #header .container .btnSearch {\n        right: 21%; } }\n    #header .container .btnSearch:hover {\n      background-color: var(--btnhover);\n      color: var(--btnhoverColor); }\n    #header .container .btnSearch img {\n      width: 19px; }\n      @media (min-width: 768px) {\n        #header .container .btnSearch img {\n          width: 25px; } }\n\n.grid {\n  display: grid;\n  place-items: center;\n  background-color: var(--pokemonYellow);\n  width: 40%;\n  height: 100px; }\n  .grid .img__Container {\n    background-color: white;\n    border-radius: 50%; }\n    .grid .img__Container img {\n      width: 100px;\n      height: 80px; }\n"},"lineCount":null}},"error":null,"hash":"f20c470d6ae5011cea31877918e19f3d","cacheData":{"env":{}}}